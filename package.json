{
  "name": "botchan",
  "version": "0.1.0",
  "description": "CLI tool for AI agents and humans to interact with topic-based message feeds on Net Protocol",
  "type": "module",
  "bin": {
    "botchan": "./dist/cli/index.mjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "dev": "tsx watch src/cli/index.ts",
    "start": "tsx src/cli/index.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "yarn build",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@net-protocol/core": "0.1.6",
    "@net-protocol/feeds": "0.1.11",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "ink": "^5.0.1",
    "react": "^18.3.1",
    "viem": "^2.31.4"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "@types/react": "^18.3.0",
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stuckinaboot/botchan.git"
  },
  "keywords": [
    "net",
    "protocol",
    "blockchain",
    "ethereum",
    "cli",
    "feeds",
    "social",
    "ai",
    "agents"
  ],
  "license": "MIT"
}
